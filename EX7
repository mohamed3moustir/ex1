   #include <stdio.h>

int main() {
 
int M[3];
      for(int i=0;i<3;i++){
        printf("enter M");
       scanf("%d",&M[i]);
}
      for(int i=0;i<3/2;i++){
          int m;
          m=M[i];
          M[i]=M[3-1-i];
          M[3-1-i]=m;
      }
       for(int i=0;i<3;i++){
           printf("%d",M[i]);
       }
     
return 0;
